pipeline {
  agent any
  tools {
    maven 'maven-3.8.6' 
  }
  environment{
    PATH = "/opt/maven3/bin:$PATH"
  }
  stages {
    stage ("Git Checkout") {
      steps {
          git credentialsId: 'MAVEN-PROJECT', url: ' https://github.com/rama100000/myrepo.git'
      }
    }
    stage ('build') {
      steps {
          sh  "mvn clean package" 
        }
      }
    }
  }
}
